<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2000"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="40dp"
            motion:layout_constraintStart_toStartOf="@+id/textViewT"
            android:layout_width="40dp"
            motion:layout_constraintTop_toBottomOf="@+id/textViewT"
            android:id="@+id/textViewI" />
        <Constraint
            android:layout_height="40dp"
            android:layout_marginTop="204dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="40dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:id="@+id/textViewM" />
        <Constraint
            android:layout_marginEnd="4dp"
            android:layout_height="40dp"
            android:layout_marginTop="204dp"
            android:layout_marginRight="4dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            android:id="@+id/textViewL" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewM"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewO"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewT"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewI"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewO2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewN"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            android:id="@+id/textViewL"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewA"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewY"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewO3"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewU"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewT2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/middle" >
        <Constraint
            android:layout_height="40dp"
            android:layout_marginTop="344dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="40dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="100dp"
            android:layout_marginStart="100dp"
            android:id="@+id/textViewM" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewO"
            motion:layout_constraintStart_toEndOf="@+id/textViewM"
            motion:layout_constraintTop_toTopOf="@+id/textViewM" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewT"
            motion:layout_constraintStart_toEndOf="@+id/textViewO"
            motion:layout_constraintTop_toTopOf="@+id/textViewO" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewI"
            motion:layout_constraintStart_toEndOf="@+id/textViewT"
            motion:layout_constraintTop_toTopOf="@+id/textViewT" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewO2"
            motion:layout_constraintStart_toEndOf="@+id/textViewI"
            motion:layout_constraintTop_toTopOf="@+id/textViewI" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewN"
            motion:layout_constraintTop_toTopOf="@+id/textViewO2"
            motion:layout_constraintStart_toEndOf="@+id/textViewO2" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewL"
            motion:layout_constraintStart_toStartOf="@+id/textViewM"
            motion:layout_constraintTop_toBottomOf="@+id/textViewM" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewA"
            motion:layout_constraintTop_toTopOf="@+id/textViewL"
            motion:layout_constraintStart_toEndOf="@+id/textViewL" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewY"
            motion:layout_constraintStart_toEndOf="@+id/textViewA"
            motion:layout_constraintTop_toTopOf="@+id/textViewA" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewO3"
            motion:layout_constraintStart_toEndOf="@+id/textViewY"
            motion:layout_constraintTop_toTopOf="@+id/textViewY" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewU"
            motion:layout_constraintTop_toTopOf="@+id/textViewO3"
            motion:layout_constraintStart_toEndOf="@+id/textViewO3" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/textViewT2"
            motion:layout_constraintTop_toTopOf="@+id/textViewU"
            motion:layout_constraintStart_toEndOf="@+id/textViewU" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/end"
        motion:constraintSetEnd="@+id/middle"
        motion:autoTransition="animateToEnd"
        motion:duration="2000" />
</MotionScene>